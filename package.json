    "@fortawesome/react-fontawesome": "^0.1.8",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "firebase": "^7.8.0",
    "firebaseui": "^4.4.0",
    "jest-cli": "^25.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^1.1.2"
  }
